
  <main class="">
    <div class="p-4 md:mx-10">
      <div class="flex justify-between">
        <div class="w-full lg:w-7/12">
           <div class="relative">
            <%= link_to users_profiles_path, class:"p-4" do %>
              <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" viewBox="0 0 12 12" fill="none">
                <path fill-rule="evenodd" clip-rule="evenodd"
                  d="M0.5 6C0.5 5.72386 0.723858 5.5 1 5.5H11C11.2761 5.5 11.5 5.72386 11.5 6C11.5 6.27614 11.2761 6.5 11 6.5H1C0.723858 6.5 0.5 6.27614 0.5 6Z"
                  fill="black"></path>
                <path fill-rule="evenodd" clip-rule="evenodd"
                  d="M6.35355 11.3536C6.15829 11.5488 5.84171 11.5488 5.64645 11.3536L0.646448 6.35355C0.451186 6.15829 0.451186 5.84171 0.646448 5.64645L5.64645 0.646448C5.84171 0.451186 6.15829 0.451186 6.35355 0.646448C6.54882 0.84171 6.54882 1.15829 6.35355 1.35355L1.70711 6L6.35355 10.6464C6.54882 10.8417 6.54882 11.1583 6.35355 11.3536Z"
                  fill="black"></path>
              </svg>
            <% end %>
          </div>

          <h2 class="font-medium md:font-thin text-2xl md:text-3xl mb-3"> What types of sessions do you offer? </h2>
          <p class="text-gray-700">Select all that apply to you.</p>

          <%= form_with(model: @session_type, url: users_session_type_path(), method: :patch) do |form| %>
            <div class="grid grid-cols-2 md:grid-cols-4 gap-2 md:gap-4 mt-8" data-controller="session-type">


              <div class="flex flex-col items-center gap-3 p-4 border rounded-xl cursor-pointer <%= 'bg-[#B7BAA7]' if @session_type.branding %>" data-action="click->session-type#toggleBranding" data-session-type-target="branding">
                <div class="h-14 w-14 rounded-full overflow-hidden bg-gray-300 flex justify-center items-center">
                  <img class="h-14 w-14 object-contain object-top" src="https://images.unsplash.com/photo-1472099645785-5658abf4ff4e?ixlib=rb-1.2.1&amp;ixid=eyJhcHBfaWQiOjEyMDd9&amp;auto=format&amp;fit=facearea&amp;facepad=2&amp;w=256&amp;h=256&amp;q=80" alt="">
                </div>
                <p class="uppercase mt-1 text-sm text-gray-500 dark:text-gray-300 hover:text-black">Branding</p>
                <%= form.hidden_field :branding, class: 'hidden-branding-checkbox' %>

              </div>

              <div class="flex flex-col items-center gap-3 p-4 border rounded-xl cursor-pointer <%= 'bg-[#B7BAA7]' if @session_type.boudoir %>" data-action="click->session-type#toggleBoudoir" data-session-type-target="boudoir">
